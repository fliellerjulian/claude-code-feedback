{
    "name": "claude-code-feedback",
    "displayName": "Claude Code Diff Feedback",
    "description": "Send diff feedback directly to Claude Code",
    "version": "0.0.1",
    "engines": {
      "vscode": "^1.85.0"
    },
    "categories": ["Other"],
    "activationEvents": ["onStartupFinished"],
    "extensionDependencies": ["vscode.git"],
    "main": "./out/extension.js",
    "contributes": {
      "commands": [
        {
          "command": "claude-code-feedback.send",
          "title": "Send Feedback to Claude Code"
        }
      ]
    },
    "scripts": {
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./",
      "package": "vsce package"
    },
    "devDependencies": {
      "@types/vscode": "^1.85.0",
      "@types/node": "^20.0.0",
      "typescript": "^5.3.0"
    }
  }